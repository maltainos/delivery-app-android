<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.mz.myrestaurante">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyRestaurante">
        <activity android:name=".ui.admin.FormProductoActivity"></activity>
        <activity android:name=".ui.LoginActivity">
        </activity>
        <activity android:name=".ui.admin.ProductoAdminActivity" />
        <activity android:name=".ui.single.ProductoSingleActivity" />
        <activity android:name=".ui.FormRestauranteActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>